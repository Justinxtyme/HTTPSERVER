;returns 1 if no machines are busy

NONE  LDI  R1, VEC ;busyness ve
      LD   R2, MASK  ; 
      LD   R3, ASCI
      AND  R0, R0, #0
      AND  R2, R1, R2 ;check busy
      BRz  FREE

BUSY  AND  R2, R2, #0
      ADD  R0, R0, R2;LD to print
      ADD  R0, R0, R3;conv asci
      TRAP x21
      RET
      


FREE  ADD  R2, R2, #1
      ADD  R0, R0, R2;LD to print
      ADD  R0, R0, R3;conv asci
      TRAP x21
      RET

VEC   .FILL x4001
MASK  .FILL x00FF
RESLT .BLKW #1
ASCI  .FILL x0030